    <template id="event_management_pickup" name="event management pickup">
        <section class="pt40 pb40 event_management_pickup o_colored_level">
            <div class="" id="event_management_pickup">
                <div class="">
                    <button class="btn text-white w-100 ev_pickup_button">
                        Pick up
                        <span class="float-right">
                            <i class="fa fa-angle-down ev_button_angle_down"/>
                        </span>
                    </button>
                    <div class="ev_pickup_container position-absolute w-100 mt-2">
                        <input type="text" class="ev_pickup_input w-100"/>
                        <div class="ev_pick_up_address">

                        </div>
                    </div>
                </div>
            </div>
        </section>
    </template>
    <template id="event_management_delivery" name="event management delivery">
