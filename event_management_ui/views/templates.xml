<odoo>

    <template id="event_management_home_landing" name="event management home landing carousal">
        <section class="s_carousel_wrapper" data-vxml="001">
            <div id="myCarousel" class="s_carousel s_carousel_default carousel slide" data-interval="10000">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"/>
                    <li data-target="#myCarousel" data-slide-to="1"/>
                    <li data-target="#myCarousel" data-slide-to="2"/>
                </ol>
                <!-- Content -->
                <div class="carousel-inner">
                    <!-- #01 -->
                    <div class="carousel-item oe_img_bg pt128 pb128"
                         style="background-image: url('/web/image/website.s_carousel_default_image_1');"
                         data-name="Slide">
                        <div class="container">
                            <div class="row content">
                                <div class="carousel-content text-center col-lg-6 offset-lg-6">
                                    <!--                                <h2><font style="font-size: 62px; background-color: rgb(255, 255, 255);">Edit this title</font></h2>-->
                                    <!--                                <h4><font style="background-color: rgb(255, 255, 255);">Good writing is simple, but not simplistic.</font></h4>-->
                                    <img width="300" height="150"/>
                                    <p>
                                        <br/>
                                    </p>
                                    <p>Good copy starts with understanding how your product or service helps your
                                        customers. Simple words communicate better than big words and pompous language.
                                    </p>
                                    <div class="row">
                                        <div class="col enable_pickup">
                                            <t t-call="event_management_ui.event_management_pickup"/>
                                        </div>
                                        <div class="col enable_delivery">
                                            <t t-call="event_management_ui.event_management_delivery"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- #02 -->
                    <div class="carousel-item oe_img_bg pt128 pb128"
                         style="background-image: url('/web/image/website.s_carousel_default_image_2');"
                         data-name="Slide">
                        <div class="container">
                            <div class="row content">
                                <div class="carousel-content text-center col-lg-6 offset-lg-6">
