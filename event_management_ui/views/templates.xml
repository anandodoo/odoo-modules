                                        <div class="col enable_delivery">
                                            <t t-call="event_management_ui.event_management_delivery"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Controls -->
                <a class="carousel-control-prev o_not_editable" contenteditable="false" href="#myCarousel"
                   data-slide="prev" role="img" aria-label="Previous" title="Previous">
                    <span class="carousel-control-prev-icon"/>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next o_not_editable" contenteditable="false" href="#myCarousel"
