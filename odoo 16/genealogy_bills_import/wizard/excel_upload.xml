<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="excel_upload_form" model="ir.ui.view">
        <field name="name">excel.upload.form</field>
        <field name="model">excel.upload</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="file"/>
                        </group>
                    </group>
                </sheet>
                <footer>
                    <button string="Create Entries" name="create_excel_import" type="object"
                            class="oe_highlight"/>
                    <button string="Cancel" class="btn btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>